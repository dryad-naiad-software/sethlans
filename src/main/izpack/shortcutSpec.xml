<izpack:shortcuts version="5.0"
                  xmlns:izpack="http://izpack.org/schema/shortcuts"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://izpack.org/schema/shortcuts http://izpack.org/schema/5.0/izpack-shortcuts-5.0.xsd">
    <programGroup defaultName="Sethlans" location="applications"/>

    <shortcut
            name="Sethlans @{info.version}"
            target="$INSTALL_PATH\bin\Sethlans.exe"
            desktop="yes"
            iconFile="$INSTALL_PATH\bin\Sethlans.exe"
            iconIndex="0"
            programGroup="yes"
            applications="yes"
            startMenu="yes">
        <createForPack name="Sethlans"/>
    </shortcut>
</izpack:shortcuts>
