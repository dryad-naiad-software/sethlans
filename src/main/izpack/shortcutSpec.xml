<shortcuts>
    <defaultCurrentUser/>
    <programGroup defaultName="$APP_NAME" location="startMenu"/>
    <shortcut name="$APP_NAME Uninstaller"
              target="javaw.exe"
              commandLine="-jar &quot;$INSTALL_PATH/Uninstaller/uninstaller.jar&quot;"
              description=""
              initialState="normal"
              programGroup="yes"
              desktop="no"
              applications="no"
              startMenu="no"
              startup="no"
              runAsAdministrator="true"
              iconFile="%SystemRoot%\system32\SHELL32.dll" iconIndex="31">
        <createForPack name="Sethlans Required Files"/>
    </shortcut>
    <shortcut
            name="$APP_NAME $APP_VER"
            target="$INSTALL_PATH\bin\Sethlans.exe"
            iconFile="$INSTALL_PATH\bin\Sethlans.exe"
            iconIndex="0"
            commandLine=""
            workingDirectory="$INSTALL_PATH/"
            description=""
            initialState="normal"
            programGroup="yes"
            desktop="yes"
            applications="no"
            startMenu="no"
            startup="no">
        <createForPack name="Sethlans"/>
    </shortcut>
</shortcuts>
